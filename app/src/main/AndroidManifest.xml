        <activity
            android:name=".value.ValorCriterio"
            android:screenOrientation="portrait" />
        <activity
            android:name=".result.ResultadoValorAlternativa"
            android:screenOrientation="portrait" />
        <activity
            android:name=".result.ResultadoValorCriterio"
            android:screenOrientation="portrait" />
        <activity
            android:name=".input.Alternativa"
            android:screenOrientation="portrait" >
        </activity>
        <activity
